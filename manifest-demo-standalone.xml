<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<OfficeApp xmlns="http://schemas.microsoft.com/office/appforoffice/1.1"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:type="TaskPaneApp">

  <Id>12345678-1234-1234-1234-123456789012</Id>
  <Version>1.0.0.0</Version>
  <ProviderName>Test Company</ProviderName>
  <DefaultLocale>en-US</DefaultLocale>
  <DisplayName DefaultValue="Test Word Add-in Demo"/>
  <Description DefaultValue="A simple test add-in for Word"/>

  <IconUrl DefaultValue="https://officedev.github.io/Office-Add-in-samples/Samples/hello-world/word-hello-world/assets/icon-32.png"/>
  <HighResolutionIconUrl DefaultValue="https://officedev.github.io/Office-Add-in-samples/Samples/hello-world/word-hello-world/assets/icon-64.png"/>
  <SupportUrl DefaultValue="https://github.com/mfellsbbtv/starling-word-addin"/>
  
  <AppDomains>
    <AppDomain>https://mfellsbbtv.github.io</AppDomain>
    <AppDomain>https://officedev.github.io</AppDomain>
  </AppDomains>

  <Hosts>
    <Host Name="Document"/>
  </Hosts>
  
  <DefaultSettings>
    <SourceLocation DefaultValue="https://mfellsbbtv.github.io/starling-word-addin/demo-word-compatible.html"/>
  </DefaultSettings>

  <Permissions>ReadWriteDocument</Permissions>

  <Requirements>
    <Sets DefaultMinVersion="1.1">
      <Set Name="WordApi" MinVersion="1.1"/>
    </Sets>
  </Requirements>

</OfficeApp>
